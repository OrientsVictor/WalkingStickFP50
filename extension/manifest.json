{
    "manifest_version": 2,
    "name": "WalkingStick",
    "version": "0.1",
    
    "content_scripts": [
        {
            "matches": ["https://*.bbc.com/*"],
            "js": ["news.js"]
        },
        {
            "matches": ["https://*.merriam-webster.com/*"],
            "js": ["define.js"]
        }
    ]
}